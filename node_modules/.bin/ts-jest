#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_27424d971495cc0da7868be369f9b859/node_modules/ts-jest/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_27424d971495cc0da7868be369f9b859/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_27424d971495cc0da7868be369f9b859/node_modules/ts-jest/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/ts-jest@29.3.1_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_27424d971495cc0da7868be369f9b859/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
