#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules/create-jest/bin/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules/create-jest/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules/create-jest/bin/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules/create-jest/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules:/home/rob/sources/personal_account/rpn-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules/create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../../../../create-jest@29.7.0_@types+node@22.14.0_ts-node@10.9.2_@types+node@22.14.0_typescript@5.8.3_/node_modules/create-jest/bin/create-jest.js" "$@"
fi
